	"license": "UNLICENSED",
	"private": true,
	"scripts": {
		"start": "cross-env MODULE=api node server.js",
		"test": "cross-env NODE_ENV=test NODE_APP_INSTANCE=test nyc node_modules/.bin/mocha --exit --timeout 15000",
		"lint": "node_modules/.bin/eslint . --quiet",
		"doc": "node_modules/.bin/apidoc -i modules/api/controllers/ -o apidoc/",
		"migrate-development": "set NODE_ENV=development ./node_modules/.bin/sequelize db:migrate",
		"migrate-production": "set NODE_ENV=production ./node_modules/.bin/sequelize db:migrate"
	},
