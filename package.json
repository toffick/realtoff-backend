		"cors": "^2.8.4",
		"express": "^4.15.5",
		"express-form": "^0.12.6",
		"express-session": "^1.15.5",
		"log4js": "^4.0.1",
		"moment": "^2.22.2",
		"nodemailer": "^5.1.1",
		"passport": "^0.4.0",
